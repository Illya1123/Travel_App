{
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": ["warn", {
      "arrowParens": "avoid",
      "semi": false,
      "trailingComma": "none",
      "endOfLine": "lf",
      "singleQuote": true,
      "tabWidth": 2,
      "printWidth": 120,
      "useTabs": true,
      "tsxSingleQuote": false,
      "jsxSingleQuote": false
    }]
  }
}